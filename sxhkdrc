
super + E
	pkill swm

super + Return
	urxvt

super + d
	dmenu_run
super + q
	killw $(pfw)

super + Down
	wmv 0 25 $(pfw)
super + Up
	wmv 0 -25 $(pfw)
super + Right
	wmv 25 0 $(pfw)
super + Left
	wmv -25 0 $(pfw)

super + shift + Right
	wrs 25 0 $(pfw)
super + shift + Left
	wrs -25 0 $(pfw)
super + shift + Up
	wrs 0 -25 $(pfw)
super + shift + Down
	wrs 0 25 $(pfw)


super + !button1
    wmv -a %i %i

super + Tab 
      /home/stephen/contrib/tile.sh

super + shift + Tab 
     /home/stephen/contrib/switch_grid.sh

